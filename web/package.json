{
  "name": "@planemgr/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -p web/tsconfig.json && vite build",
    "lint": "eslint --config ../eslint.config.mjs \"src/**/*.{ts,tsx}\"",
    "format": "prettier --config ../prettierrc.json --write \"src/**/*.{ts,tsx,css,md,json}\"",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@planemgr/plugin-core": "workspace:*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reactflow": "^11.11.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "vite": "^7.3.1",
    "sass": "^1.97.3"
  }
}
